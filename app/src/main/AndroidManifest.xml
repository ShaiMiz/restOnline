<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.shaiMiz.restOnline">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:layout_gravity="right"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:textAlignment="gravity"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.example.shaiMiz.restOnline.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.shaiMiz.restOnline.Register" />
        <activity android:name="com.example.shaiMiz.restOnline.Login" />
        <activity android:name="com.example.shaiMiz.restOnline.breakfast" />
        <activity android:name="com.example.shaiMiz.restOnline.appetizers" />
        <activity android:name="com.example.shaiMiz.restOnline.cold_drinks" />
        <activity android:name="com.example.shaiMiz.restOnline.desserts" />
        <activity android:name="com.example.shaiMiz.restOnline.main_dish" />
        <activity android:name="com.example.shaiMiz.restOnline.salads" />
        <activity android:name="com.example.shaiMiz.restOnline.MainPage"></activity>
    </application>

</manifest>